{
    "name": "webapp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "watch": "tsc && vite build --mode=watch",
        "serve": "vite preview",
        "lint": "eslint --color --ext .js,.jsx,.ts,.tsx src --quiet",
        "lint:fix": "eslint --color --ext .js,.jsx,.ts,.tsx src --quiet --fix",
        "prettier": "prettier --write ."
    },
    "dependencies": {
        "@ant-design/charts": "^1.4.2",
        "@ant-design/icons": "^4.7.0",
        "@ant-design/pro-card": "^1.20.19",
        "@ant-design/pro-descriptions": "^1.12.2",
        "@ant-design/pro-form": "^1.74.2",
        "@ant-design/pro-layout": "^6.38.20",
        "@ant-design/pro-list": "^1.21.81",
        "@ant-design/pro-skeleton": "^1.0.13",
        "@ant-design/pro-table": "^2.80.2",
        "antd": "^4.22.7"
    },
    "devDependencies": {
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "autoprefixer": "^10.4.8",
        "less": "^4.1.3",
        "path": "^0.12.7",
        "postcss": "^8.4.16",
        "sass": "^1.54.5",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.7.4",
        "vite": "^3.0.9",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "eslintConfig": {
        "extends": [
            "../../.eslintrc.js"
