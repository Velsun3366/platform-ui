{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "lib": ["esnext", "dom", "dom.iterable"],
        "types": [],
        "baseUrl": "."
    },
    "include": ["."],
    "exclude": ["node_modules"]
}
